{
  "compilerOptions": {
    "noEmit": true,
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "jsx": "react-jsx",
    "isolatedModules": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": ["."]
}
